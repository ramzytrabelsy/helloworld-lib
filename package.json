{
  "private": true,
  "name": "helloworld-lib",
  "version": "0.16.0",
  "engines": {
    "node": "^8.0.0 || ^10.0.0",
    "npm": "^5.0.0 || ^6.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./",
    "format": "prettier './**/*.{md,json,js,css,scss}' --write && eslint -o /dev/null --ext .js --fix ./ || true",
    "test": "NODE_ICU_DATA=$(dirname $(node-full-icu-path)) jest",
    "build": "babel --source-maps true ./src/ -d ./dist/",
    "build:watch": "babel -w --source-maps true ./src/ -d ./dist/"
  },
  "dependencies": {
    "core-js": "~3.0.1",
    "debug": "~4.1.1",
    "events": "~3.0.0",
    "intl": "~1.2.5",
    "intl-relativeformat": "~2.1.0",
    "regenerator-runtime": "~0.13.2",
    "whatwg-fetch": "~3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.4.4",
    "@babel/core": "~7.4.4",
    "@babel/preset-env": "~7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.8.0",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-plugin-import": "~2.17.2",
    "full-icu": "~1.3.0",
    "jest": "~24.8.0",
    "prettier": "~1.17.1"
  }
}
